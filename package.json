{
  "name": "theme-vitesse",
  "displayName": "Vitesse Theme",
  "version": "0.2.2",
  "description": "Vitesse theme for VS Code",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "vitesse",
    "light",
    "dark"
  ],
  "files": [
    "themes"
  ],
  "bugs": {
    "url": "https://github.com/antfu/vscode-theme-vitesse/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-theme-vitesse"
  },
  "license": "MIT",
  "publisher": "antfu",
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "vscode:prepublish": "yarn build",
    "release": "yarn build && npx bumpp --commit --tag --push --all && vsce publish --no-dependencies"
  },
  "contributes": {
    "themes": [
      {
        "label": "Vitesse Light",
        "uiTheme": "vs",
        "path": "./themes/vitesse-light.json"
      },
      {
        "label": "Vitesse Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/vitesse-dark.json"
      }
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.2",
    "@antfu/utils": "^0.0.3",
    "@types/color": "^3.0.1",
    "@types/node": "^14.14.37",
    "color": "^3.1.3",
    "eslint": "^7.23.0",
    "esno": "^0.5.0",
    "nodemon": "^2.0.7",
    "tsup": "^4.8.21",
    "typescript": "^4.2.4"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "icon": "icon.png",
  "preview": true
}
